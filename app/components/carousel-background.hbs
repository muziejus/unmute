<AnimatedContainer class="absolute top-0 left-0 w-screen h-screen sticky"
  {{did-insert this.startCarousel}}
  >
  <div class="video-image-overlay"></div>

  {{#animated-value this.activePhoto
      duration=this.transitionDuration
      rules=this.rules
      as |photo| }}
      <div class="carousel-credit">
        <p class="text-white relative bottom-0 left-0 pb-4 pl-4">
          Photo: {{photo.credit}}
        </p>
      </div>
      <div class="carousel-image">
        <img alt="alt {{photo.credit}}" src={{photo.url}} 
        class="object-cover w-full h-full">
      </div>
  {{/animated-value}}
</AnimatedContainer>
